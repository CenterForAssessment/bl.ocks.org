{
  "name": "bl.ocks.org",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "16": "bl.ocks.png"
  },
  "browser_action": {
    "default_icon": "bl.ocks.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["bl.ocks.js"]
    }
  ]
}
